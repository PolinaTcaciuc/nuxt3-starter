{
  "name": "nuxt3-starter",
  "author": {
    "name": "Polina Tcaciuc",
    "email": "polinatcaciuc@gmail.com"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "test": "vitest",
    "test:unit:run": "vitest --config vitest.config.ts --run",
    "test:unit:watch": "vitest --config vitest.config.ts --watch",
    "test:unit:changed": "vitest run --changed --coverage ",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint-staged": "lint-staged",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "stylelint": "stylelint '**/*{.css,scss,vue}'",
    "stylelint:fix": "stylelint '**/*{.css,scss,vue}' --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "nuxt": "^3.11.2",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.3.9",
    "@nuxt/test-utils": "^3.12.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@testing-library/vue": "^8.0.3",
    "@vitest/coverage-v8": "^1.5.0",
    "@vitest/ui": "^1.5.0",
    "@vue/test-utils": "^2.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^48.2.3",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "eslint-plugin-vitest": "^0.5.3",
    "eslint-plugin-vue": "^9.25.0",
    "happy-dom": "^14.7.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "playwright-core": "^1.43.1",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "stylelint": "^16.3.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.2.1",
    "vitest": "^1.5.0"
  }
}
